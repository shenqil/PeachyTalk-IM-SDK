{
  "name": "peachy-talk-im-sdk-dev",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "types": "./types/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.umd.cjs"
  },
  "scripts": {
    "build": "tsc",
    "prettier": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "proto:ts": "protoc --ts_out lib/protocolLayer/protobuf/proto --proto_path lib/protocolLayer/protobuf/proto lib/protocolLayer/protobuf/proto/*.proto",
    "rebuild": "electron-rebuild -f -a x64 -v 15.3.1 -w sqlite3"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "@types/node": "^20.12.12",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "protoc": "^1.1.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.9.4",
    "axios": "^1.6.7",
    "mqtt": "^5.5.4"
  }
}
